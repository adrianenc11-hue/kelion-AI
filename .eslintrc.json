{
    "env": {
        "node": true,
        "browser": true,
        "es2022": true
    },
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "rules": {
        "no-undef": "warn",
        "no-unused-vars": [
            "warn",
            {
                "argsIgnorePattern": "^_"
            }
        ],
        "no-redeclare": "error",
        "no-dupe-keys": "error",
        "no-duplicate-case": "error",
        "no-unreachable": "warn",
        "no-constant-condition": "warn",
        "no-empty": "warn",
        "eqeqeq": [
            "warn",
            "smart"
        ],
        "no-eval": "error",
        "no-implied-eval": "error",
        "no-new-func": "error"
    },
    "ignorePatterns": [
        "node_modules/",
        ".k1_backups/",
        "*.min.js"
    ]
}